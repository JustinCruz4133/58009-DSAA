#include <iostream>
using namespace std;

int main()
{
    int length, width, area;
    cout<< "---Welcome to Rectangle Area Calculator---\n\n";
    
    cout<< "Enter the Length of the rectangle: ";
    cin>> length;
    cout<< "Enter the Width of the rectangle: ";
    cin>> width;
    
    area = length * width;
    cout<< "\n\nThe Area of the Rectangle is "<< area;

    return 0;
}
